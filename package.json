{
  "name": "capitech-final",
  "version": "1.0.0",
  "main": "src/index.ts",
  "repository": "https://github.com/alihpss/capitech-final.git",
  "author": "alihpss <ali.hps@hotmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "rimraf dist && tsc",
    "ts.check": "tsc --project tsconfig.json",
    "add-build": "git add dist",
    "start": "node -r dotenv/config ./dist/index.js",
    "dev": "nodemon --watch 'src/' --exec 'ts-node -r dotenv/config src/index.ts' -e ts"
  },
  "pre-commit": [
    "ts.check",
    "build",
    "add-build"
  ],
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/mongoose": "^5.11.97",
    "@types/node": "^20.14.0",
    "nodemon": "^3.1.3",
    "pre-commit": "^1.2.2",
    "rimraf": "^5.0.7",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.4.1",
    "reflect-metadata": "^0.2.2"
  }
}
